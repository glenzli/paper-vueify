{
  "name": "paper-vueify",
  "version": "1.1.13",
  "description": "A vue adaptation of paper.js",
  "author": {
    "name": "glenzli",
    "email": "glenzli@outlook.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name paper-vueify ./src/lib.ts",
    "lint": "vue-cli-service lint",
    "build:demo": "cross-env DEMO_TARGET=true vue-cli-service build --dest gh-pages && vuepress build docs",
    "build:docs": "vuepress build docs",
    "serve:docs": "vuepress dev docs",
    "test": "jest"
  },
  "main": "./dist/paper-vueify.umd.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "js-corelib": "^1.1.3",
    "paper": "^0.12.11",
    "paper-vueify-datatypes": "^1.0.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-highlightjs": "^1.3.3",
    "vue-property-decorator": "^9.0.2"
  },
  "devDependencies": {
    "@types/highlight.js": "^9.12.4",
    "@types/jest": "^26.0.14",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.1.0",
    "codecov": "^3.8.0",
    "cross-env": "^7.0.2",
    "highlight.js": "^9.15.8",
    "jest": "^26.5.2",
    "jest-canvas-mock": "^2.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.6.0"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "satements": 10
      }
    }
  },
  "_id": "paper-vueify@1.1.11",
  "bugs": {
    "url": "https://github.com/glenzli/paper-vueify/issues"
  },
  "homepage": "https://github.com/glenzli/paper-vueify#readme",
  "keywords": [
    "paper",
    "vue",
    "canvas"
  ],
  "license": "MIT",
  "readme": "ERROR: No README data found!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glenzli/paper-vueify.git"
  },
  "typings": "dist/types/lib.d.ts"
}
